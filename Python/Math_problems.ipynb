   "source": [
    "求解[100,999]区间内个十百位相加能被8整除的数字的个数"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "count = 0\n",
    "for num in range(100, 999+1):\n",
    "    num3 = num // 100\n",
    "    num2 = (num-100*num3) // 10\n",
    "    num1 = (num-100*num3-10*num2)\n",
    "    \n",
    "    if (num1+num2+num3) % 8 == 0:\n",
    "        count += 1"
   ]
  },
